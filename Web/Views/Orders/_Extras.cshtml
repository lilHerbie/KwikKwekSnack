@using ClassLibrary;

@model SnackLine;

<div class="snackContainer">
    <h1>@Model.Snack.Name</h1>
    <img src="@Model.Snack.ImageUrl" />

    @{
        foreach (Extra extra in ViewBag.Extras)
        {
            
            <a asp-action="AddExtra" asp-route-id="@extra.Id">@extra.Name</a>
        }
        
    }

</div>